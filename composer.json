{
    "name": "aura/filter-intl-bundle",
    "type": "library",
    "description": "Integration bundle for Aura.Filter with Aura.Intl",
    "keywords": [ "filter", "validate", "sanitize", "intl"],
    "homepage": "https://github.com/harikt/Aura.Filter_Intl_Bundle",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Aura Contributors",
            "homepage": "https://github.com/harikt/Aura.Filter_Intl_Bundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "aura/intl": "1.*",
        "aura/filter": "1.*"
    },
    "autoload": {        
        "psr-4": {
            "Aura\\Filter_Intl_Bundle\\": "src/",
            "Aura\\Filter_Intl_Bundle\\_Config\\": "config/"
        }
    },
    "extra": {
        "aura": {
            "type": "library",
            "config": {
                "common": "Aura\\Filter_Intl_Bundle\\_Config\\Common"                
            }
        }
    }
}
